{
    "name": "nilportugues/cache",
    "description": "Cache layer for PHP applications using the on Chain of responsability pattern",
    "keywords": [
        "cache", "performance", "sphinx", "sql", "sphinxql", "elasticsearch", "file", "filesystem", "system", "chainable",
        "delegation", "memcached", "mysql", "mongo", "mongodb", "redis", "predis", "nil portugues", "nilportugues"
    ],
    "type": "library",
    "homepage": "http://nilportugues.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Nil PortuguÃ©s",
            "email": "contact@nilportugues.com"
        }
    ],
    "support": {
        "email": "contact@nilportugues.com",
        "issues": "https://github.com/nilopc/cache-layer/issues"
    },
    "require": {
        "php": ">=5.5",
        "predis/predis": "1.1.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "fiunchinho/phpunit-randomizer": "1.0.*@dev",
        "fabpot/php-cs-fixer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "NilPortugues\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NilPortugues\\Tests\\Cache\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "ext-redis": "Native redis support for php.",
        "ext-memcached": "Native memcached support for php."
    }
}
